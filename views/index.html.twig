{% extends "layout.html.twig" %} {% block title %}Home{% endblock %} {% block content %}

<body>
    <div class="container">
        <div class="row">

            {% for category in categorys %}
            <div class="col-xs-4 col-sm-4 col-md-3">
                <h1><a class="thumbnail" href="{{ path('category', { 'cat': category.name }) }}"><img src="{{app.request.basepath}}/images/{{category.id}}.jpg"></a></h1>
            </div>
            {% endfor %}

        </div>
    </div>
</body>
{% endblock %}